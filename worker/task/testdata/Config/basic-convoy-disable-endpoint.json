{
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "abc"
        }
    },
    "server": {
        "http": {
            "port": 80
        }
    },
    "auth": {
        "file": {
            "basic": [
                {
                    "username": "test",
                    "password": "test",
                    "role": {
                        "type": "admin",
                        "groups": [
                            "sendcash-pay"
                        ]
                    }
                }
            ]
        }
    },
    "signature": {
        "header": "X-Company-Event-WebHook-Signature",
        "hash": "SHA256"
    },
    "strategy": {
        "type": "default",
        "intervalSeconds": 125,
        "retryLimit": 15,
        "disable_endpoint": true
    },
    "smtp": {
        "provider": "sendgrid",
        "url": "smtp.sendgrid.net",
        "port": 2525,
        "username": "apikey",
        "password": "<api-key-from-sendgrid>",
        "from": "support@frain.dev"
    }
}