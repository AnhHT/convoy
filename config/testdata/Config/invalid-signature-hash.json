{
    "database": {
        "dsn": "mongodb://inside-config-file"
    },
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "redis://localhost:8379"
        }
    },
    "server": {
        "http": {
            "port": 80
        }
    },
    "signature": {
        "hash": "SHA100"
    },
    "strategy": {
        "type": "default",
        "intervalSeconds": 125,
        "retryLimit": 15
    }
}
